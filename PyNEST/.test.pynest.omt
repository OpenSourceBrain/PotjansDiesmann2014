target: example.py
engine: PyNEST
mep: .test.pynest.mep
experiments:
  
  # Note: test should start at 200ms, to avoid rapid spiking at start!!! 
  spike_rate_l23e_0:
    observables:
      spike rate:
        spiketimes file: 
          path: data/spike_detector-7714-0.gdf
          format: ID_TIME
          scaling: 0.001
          ids: '*'
        method: duration based
        start_time: 0.2
        end_time: 1
        tolerance: 0.00
